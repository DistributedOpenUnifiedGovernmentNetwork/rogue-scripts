#!/bin/bash

DEV_MODE=True
GEONODE_REPO_URL=https://github.com/jj0hns0n/geonode.git
GEONODE_BRANCH=upload-work
GSCONFIG_REPO_URL=https://github.com/jj0hns0n/gsconfig.py.git
GSCONFIG_BRANCH=data_store_type
DOWNSTREAM_PROJECT_URL=https://github.com/ROGUE-JCTD/rogue_geonode.git
DOWNSTREAM_PROJECT_BRANCH=master
GEONODE_DEPENDENCIES="uwsgi psycopg2"
DOWNSTREAM_PROJECT_NAME=rogue_geonode
GEOGIT_HOME_DIR=/home/rogue/dev/GeoGit
VIRTUAL_ENV=/var/lib/geonode
STATIC_DIR=$VIRTUAL_ENV/$DOWNSTREAM_PROJECT_NAME/$DOWNSTREAM_PROJECT_NAME/static_root
SITE_URL=192.168.10.144
GEOSERVER_BASE_URL=192.168.10.144/geoserver

printf "DEV_MODE=$DEV_MODE\n";
printf "GEONODE_REPO_URL=$GEONODE_REPO_URL\n";
printf "GEONODE_BRANCH=$GSCONFIG_REPO_URL\n";
printf "GSCONFIG_REPO_URL=$GSCONFIG_REPO_URL\n";
printf "GSCONFIG_BRANCH=$GSCONFIG_BRANCH\n";
printf "DOWNSTREAM_PROJECT_URL=$DOWNSTREAM_PROJECT_URL\n";
printf "DOWNSTREAM_PROJECT_BRANCH=$DOWNSTREAM_PROJECT_BRANCH\n";
printf "GEONODE_DEPENDENCIES=$GEONODE_DEPENDENCIES\n";
printf "DOWNSTREAM_PROJECT_NAME=$DOWNSTREAM_PROJECT_NAME\n";
printf "GEOGIT_HOME_DIR=$GEOGIT_HOME_DIR\n";
printf "VIRTUAL_ENV=$VIRTUAL_ENV\n";
printf "STATIC_DIR=$STATIC_DIR\n";
printf "SITE_URL=$SITE_URL\n";
printf "GEOSERVER_BASE_URL=$GEOSERVER_BASE_URL\n\n";
